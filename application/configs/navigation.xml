<?xml version="1.0" encoding="UTF-8"?>
<configdata>
	<nav1>
		<accueil>
			<label>Accueil</label>
			<module>default</module>
			<controller>index</controller>
			<action>index</action>
		</accueil>
		<contact>
			<label>Contact</label>
			<module>default</module>
			<controller>index</controller>
			<action>contact</action>
		</contact>
	</nav1>
	<nav2>
		<panier>
			<id>nav-panier</id>
			<icon>icon-shopping-cart icon-white</icon>
			<label>Mon panier</label>
			<module>default</module>
			<controller>panier</controller>
			<action>index</action>
		</panier>	
		<compte>
			<id>nav-compte</id>
			<icon>icon-user icon-white</icon>
			<label>Mon compte</label>
			<module>default</module>
			<controller>auth</controller>
			<action>index</action>
			<pages>
				<compte>
					<icon>icon-wrench</icon>
					<separator></separator>
					<label>Mon compte</label>
					<module>default</module>
					<controller>auth</controller>
					<action>index</action>
				</compte>
				<deconnexion>
					<icon>icon-off</icon>
					<label>Déconnexion</label>
					<module>default</module>
					<controller>auth</controller>
					<action>logout</action>
				</deconnexion>
			</pages>
		</compte>
	</nav2>
	<sidebar>
		<tous>
			<label>Tous les tableaux</label>
			<module>default</module>
			<controller>article</controller>
			<action>index</action>
			<params>
				<tableau>tout</tableau>
			</params>
		</tous>
		<nouveautes>
			<label>Nouveautés</label>
			<module>default</module>
			<controller>article</controller>
			<action>index</action>
			<params>
				<tableau>nouveau</tableau>
			</params>
		</nouveautes>
		<populaires>
			<label>Les plus populaires</label>
			<module>default</module>
			<controller>article</controller>
			<action>index</action>
			<params>
				<tableau>populaire</tableau>
			</params>
		</populaires>
		<themes>
			<label>Par thème</label>
			<module>default</module>
			<controller>article</controller>
			<action>index</action>
			<params>
				<theme>tout</theme>
			</params>
		</themes>
		<dimensions>
			<label>Par dimension</label>
			<module>default</module>
			<controller>article</controller>
			<action>index</action>
			<params>
				<dimension>tout</dimension>
			</params>
		</dimensions>
	</sidebar>
